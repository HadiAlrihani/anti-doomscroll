{
    "name": "doomscroll doctor",
    "description": "a gentle reminder to step away from the void.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "dashboard.html"
    },
    "action": {
        "default_popup": "popup.html"
    }
}